{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/web-common/(.*)": "<rootDir>/../libs/web-common/src/$1",
    "@libs/web-common": "<rootDir>/../libs/web-common/src",
    "@libs/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@libs/logger": "<rootDir>/../libs/logger/src",
    "@libs/entity/(.*)": "<rootDir>/../libs/entity/src/$1",
    "@libs/entity": "<rootDir>/../libs/entity/src"
  }
}
